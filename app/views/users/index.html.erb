<div class="content">
<% @users.each do |user| %>
<table class="table table-hover table-striped">
	<thead>
		<tr>
			<th>写真</th>
			<th>種目</th>
			<th>名前</th>
			<th>都道府県</th>
			<th>LINE</th>
			<th>メールアドレス</th>
		</tr>
	</thead>
	<tbody>
		<tr>
			<td><%= attachment_image_tag user, :image, size: "50x50", fallback:"img5.png", class: "img-circle pull-left profile-thumb" %></td>
			<td><%= user.competition %></td>
			<td><%= user.name %></td>
			<td><%= user.address %></td>
			<td><%= user.line_id %></td>
			<td><%= user.email %></td>
		</tr>
	</tbody>
</table>
</div>

<% end %>
